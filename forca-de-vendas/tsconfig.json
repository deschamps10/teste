{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "#app": ["./node_modules/nuxt3/dist/app"]
    }
  },
  "include": [
    "nuxt.config.ts",  // Certifique-se de que o arquivo de configuração do Nuxt é incluído aqui
    "plugins/**/*.ts",  // Se você estiver usando plugins em TypeScript
    "pages/**/*.ts",
    "components/**/*.ts"
  ]
}
